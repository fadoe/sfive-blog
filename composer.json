{
    "name": "fadoe/sfive-blog",
    "description": "Fork of Serendipity Blogging Engine",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Falk Doering",
            "homepage": "https://github.com/fadoe/sfive-blog"
        }
    ],
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net",
            "vendor-alias": "pear-pear"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "smarty/smarty": "~3.1",
        "simplepie/simplepie": "1.3.*",
        "pear-pear/Pear": "1.9.*",
        "pear-pear/HTTP_Request": "1.4.*",
        "pear-pear/Xml_Rpc": "1.5.*",
        "zendframework/zend-filter": "~2.2",
        "zf-commons/zfc-base": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Sfive": "src/library/",
            "SfiveTest": "test/"
        },
        "classmap": [
        ]
    },
    "config": {
        "vendor-dir": "src/vendor",
        "bin-dir": "bin"
    }
}
